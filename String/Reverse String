// Problem Link -> https://leetcode.com/problems/reverse-string/

// CODE

class Solution {
    public void reverseString(char[] s) {
        int li = 0;
        int ri = s.length - 1;
        while(li < ri) {
            char ch = s[li];
            s[li] = s[ri];
            s[ri] = ch;
            li++;
            ri--;
        }
        
    }
}
